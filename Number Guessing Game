import random

magic_num= random.randrange(0,21)                               #Generate random number between 0 to 20

while True:
    guess_num = int(input("Guess Magic Number between 0-20 : "))
    
    if guess_num==magic_num:                                        #Check whether generated number & number guessed by user are equal
        print("Your Guess is Correct!")
        break
    else:
        print("Try Again!")